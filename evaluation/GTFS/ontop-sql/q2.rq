SELECT v1."stop_desc" AS "stop_desc2m34",
       v1."stop_id" AS "stop_id1m30",
       v1."stop_lat" AS "stop_lat1m33",
       v1."stop_lon" AS "stop_lon1m36",
       v1."wheelchair_boarding" AS "wheelchair_boarding2m38"
FROM "stops" v1
WHERE ((CAST(v1."stop_lat" AS BIGINT) = 518)
       AND v1."stop_lat" IS NOT NULL
       AND v1."stop_lon" IS NOT NULL)
